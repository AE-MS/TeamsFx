{
  "name": "@microsoft/teamsfx-bot",
  "version": "0.1.0",
  "description": "Microsoft Teams Framework for Bot & Message Extension.",
  "main": "dist/index.node.cjs.js",
  "module": "dist/index.esm2017.mjs",
  "types": "types/teamsfx.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c && api-extractor run --local && npm run api-markdown",
    "prepare": "npm run build",
    "api-markdown": "api-documenter generate -i temp -o ../../docs/sdk-bot",
    "clean": "rimraf dist temp types",
    "check-format": "prettier --list-different \"src/**/*.ts\" \"test/**/*.ts\" \"*.{js,json}\" \"config/*.js\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"*.{js,json}\" \"config/*.js\"",
    "lint:staged": "lint-staged",
    "lint": "eslint \"**/*.ts\"",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "nyc --no-clean -- mocha 'test/unit/*.spec.ts' --file src/index.ts -r config/mocha.env.ts --config config/mocharc.node.js",
    "test:e2e": "nyc --reporter lcovonly -- mocha 'test/e2e/*.spec.ts' --file src/index.ts -r config/mocha.env.ts --config config/mocharc.node.js",
    "precommit": "lint-staged",
    "version": "../../.github/scripts/pkg-version.sh templates",
    "postversion": "../../.github/scripts/pkg-postversion.sh templates"
  },
  "files": [
    "dist/*js",
    "dist/*js.map",
    "types/teamsfx-bot.d.ts",
    "README.md",
    "LICENSE"
  ],
  "repository": "https://github.com/OfficeDev/TeamsFx",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "homepage": "https://github.com/OfficeDev/TeamsFx",
  "sideEffects": false,
  "dependencies": {
    "@azure/core-auth": "^1.3.2",
    "@microsoft/teamsfx": "file:../sdk",
    "botbuilder": ">=4.15.0 <5.0.0",
    "botbuilder-dialogs": ">=4.15.0 <5.0.0",
    "botframework-schema": ">=4.15.0 <5.0.0",
    "jwt-decode": "^3.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.68",
    "@microsoft/api-extractor": "^7.18.19",
    "@rollup/plugin-json": "^4.1.0",
    "@shared/eslint-config": "file:../eslint-config",
    "@shared/prettier-config": "file:../prettier-config",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.6",
    "@types/url-join": "^4.0.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.1.7",
    "mocha": "^9.1.3",
    "mocked-env": "^1.3.5",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "url-join": "^4.0.1"
  },
  "gitHead": "7d60c0765c0ea8c023a26c10d1c93001c597afbb",
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{js,jsx,css,ts,tsx}": [
      "npx eslint --cache --fix --quiet"
    ]
  }
}
